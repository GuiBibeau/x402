{
  "name": "express",
  "type": "server",
  "language": "typescript",
  "endpoints": [
    {
      "path": "/protected",
      "method": "GET",
      "description": "Protected endpoint requiring payment",
      "requiresPayment": true
    },
    {
      "path": "/health",
      "method": "GET",
      "description": "Health check endpoint",
      "requiresPayment": false
    },
    {
      "path": "/close",
      "method": "POST",
      "description": "Graceful shutdown endpoint",
      "requiresPayment": false
    }
  ],
  "environment": {
    "required": [
      "PORT",
      "USE_CDP_FACILITATOR",
      "NETWORK",
      "ADDRESS"
    ],
    "optional": [
      "CDP_API_KEY_ID",
      "CDP_API_KEY_SECRET"
    ]
  }
}